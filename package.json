{
  "name": "c9-back",
  "version": "1.0.0",
  "description": "Backend para C9",
  "main": "index.js",
  "scripts": {
    "start": "NODE_ENV=production node ./index.js --inspect",
    "dev": " nodemon ./index.js",
    "db:migrate": "npx sequelize-cli db:migrate",
    "db:migrate:generate": "npx sequelize-cli migration:generate --name",
    "reset:all": "npx sequelize-cli db:drop && npx sequelize-cli db:create && npx sequelize-cli db:migrate && npx sequelize-cli db:seed:all",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dlab-team/c9-back.git"
  },
  "keywords": [],
  "author": "",
