  "bugs": {
    "url": "https://github.com/dlab-team/c9-back/issues"
  },
  "homepage": "https://github.com/dlab-team/c9-back#readme",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "http-status": "^1.6.2",
    "jsonwebtoken": "^9.0.0",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.9.1",
    "nodemon": "^2.0.22",
    "path": "^0.12.7",
    "pg": "^8.10.0",
    "sequelize": "^6.31.0",
    "sequelize-cli": "^6.6.0"
  }
